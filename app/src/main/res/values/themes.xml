<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.ParcialTP3" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/primarydark</item>
        <item name="colorAccent">@color/accent</item>
        <item name="android:textColor">@color/dark_primary_dark</item>


    </style>

    <style name="Theme.ParcialTP3" parent="Base.Theme.ParcialTP3" />

    <style name="Base.Theme.ParcialTP3.Dark" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/dark_primary_dark</item>
        <item name="colorAccent">@color/dark_accent</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Theme.ParcialTP3.Dark" parent="Base.Theme.ParcialTP3.Dark" />


    <style name="custom_card_view" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">40dp</item>
        <item name="cornerSizeTopLeft">40dp</item>
    </style>
</resources>